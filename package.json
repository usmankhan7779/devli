{
  "name": "lineups",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:universal": "run-s build:client build:aot build:server:prod server",
    "start:universal--stats": "run-s build:client-with-stats-output build:aot build:server:prod server",
    "show-stats-report": "webpack-bundle-analyzer ./dist/browser/stats.json",
    "build:client": "npm run max-heap run lineups:build -- --configuration=production",
    "build:client-with-stats-output": "npm run build:client -- --stats-json",
    "build:aot": "npm run max-heap run lineups:server:production",
    "build:server:dev": "NODE_ENV=dev node --max_old_space_size=4096 node_modules/.bin/webpack -p --progress --colors",
    "build:server:stage": "NODE_ENV=stage node --max_old_space_size=4096 node_modules/.bin/webpack -p --progress --colors",
    "build:server:prod": "NODE_ENV=prod node --max_old_space_size=4096 node_modules/.bin/webpack -p --progress --colors",
    "start:universal-dev": "run-s build:dev-client build:dev-aot build:server:dev server",
    "build:dev-client": "npm run max-heap run lineups:build -- --configuration=dev",
    "build:dev-aot": "npm run max-heap run lineups:server -- --configuration=dev",
    "start:universal-stage": "run-s build:stage-client build:stage-aot build:server:stage server",
    "build:stage-client": "npm run max-heap run lineups:build -- --configuration=stage",
    "build:stage-aot": "npm run max-heap run lineups:server -- --configuration=stage",
    "max-heap": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "repack:server": "clear && run-s build:server:prod server",
    "rebuild:server": "clear && run-s build:aot build:server:prod server",
    "server": "node dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sprites:mlb": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite.svg' --view-dest='src/assets/images/mlb/logos/' 'src/./assets/images/mlb/logos/**/*.svg'",
    "sprites:mlb:bordered": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-bordered.svg' --view-dest='src/assets/images/mlb/logos/' 'src/./assets/images/mlb/logos/bordered/*.svg'",
    "sprites:mlb:white": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-white.svg' --view-dest='src/assets/images/mlb/logos/' 'src/./assets/images/mlb/logos/white/*.svg'",
    "sprites:nfl": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite.svg' --view-dest='src/assets/images/nfl/logos/' 'src/./assets/images/nfl/logos/**/*.svg'",
    "sprites:nfl:bordered": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-bordered.svg' --view-dest='src/assets/images/nfl/logos/' 'src/./assets/images/nfl/logos/bordered/*.svg'",
    "sprites:nfl:white": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-white.svg' --view-dest='src/assets/images/nfl/logos/' 'src/./assets/images/nfl/logos/white/*.svg'",
    "sprites:nba": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite.svg' --view-dest='src/assets/images/nba/logos/' 'src/./assets/images/nba/logos/**/*.svg'",
    "sprites:nba:white": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-white.svg' --view-dest='src/assets/images/nba/logos/' 'src/./assets/images/nba/logos/white/*.svg'",
    "sprites:nba:bordered": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-bordered.svg' --view-dest='src/assets/images/nba/logos/' 'src/./assets/images/nba/logos/bordered/*.svg'",
    "sprites:nhl": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite.svg' --view-dest='src/assets/images/nhl/logos/' 'src/./assets/images/nhl/logos/**/*.svg'",
    "sprites:nhl:white": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-white.svg' --view-dest='src/assets/images/nhl/logos/' 'src/./assets/images/nhl/logos/white/*.svg'",
    "sprites:nhl:bordered": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-bordered.svg' --view-dest='src/assets/images/nhl/logos/' 'src/./assets/images/nhl/logos/bordered/*.svg'",
    "sprites:cfb:white": "svg-sprite --view --view-layout='diagonal' --view-bust=false --view-sprite='sprite-white.svg' --view-dest='src/assets/images/cfb/logos/' 'src/./assets/images/cfb/logos/white/*.svg'"
  },
  "resolutions": {
    "ajv": "6.8.1"
